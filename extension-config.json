{
  "version": "0.1.0",
  "id": "@shopgate-project/bolt-checkout",
  "components": [
    {
      "id": "BoltCheckoutButton",
      "path": "frontend/portals/CartCheckoutButton/index.jsx",
      "target": "cart.checkout-button",
      "type": "portals"
    },
    {
      "id": "Subscribers",
      "type": "subscribers",
      "path": "frontend/subscriptions"
    }
  ],
  "configuration": {
    "boldApiBaseUrl": {
      "type": "admin",
      "destination": "backend",
      "default": "https://api-sandbox.bolt.com/",
      "params": {
        "type": "text",
        "label": "bolt pay api base url",
        "required": true
      }
    },
    "boldApiKey": {
      "type": "admin",
      "destination": "backend",
      "default": "bff75639efbf6e38519994d052f5b30f15f603a647123b1d4a40ea5592d61d02",
      "params": {
        "type": "text",
        "label": "bolt pay api key",
        "required": true
      }
    }
    "boltPublishableKey": {
      "type": "admin",
      "destination": "frontend",
      "default": "_WbB6Np-UkMq.TtSBR6r4dtds.da5c8d172bc679cf431a8e77cb08a01552208329782a36a6b3d90499def02728",
      "params": {
        "required": true,
        "type": "string",
        "label": "Bolt Publishable Key"
      }
    },
    "cdnUrl": {
      "type": "admin",
      "destination": "frontend",
      "default": "connect-sandbox.bolt",
      "params": {
        "required": true,
        "type": "string",
        "label": "Bolt CDN url"
      }
    },
    "shoppingCart": {
      "type": "admin",
      "destination": "frontend",
      "default": "Magento",
      "params": {
        "required": true,
        "type": "string",
        "label": "Merchant's shopping cart provider"
      }
    }
  }
}
